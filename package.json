{
	"name": "gatsby-remark-colspan",
	"version": "0.0.1",
	"description": "Gatsby Transformer Remark plugin to recognize colspan in markdown",
	"main": "index.js",
	"dependencies": {
		"babel-runtime": "^6.26.0",
		"rehype-stringify": "^5.0.0",
		"remark-grid-tables": "^1.2.0",
		"remark-parse": "^6.0.3",
		"remark-rehype": "^4.0.0",
		"unified": "^7.0.2",
		"unist-util-find-before": "^2.0.2",
		"unist-util-remove": "^1.0.1",
		"unist-util-visit": "^1.1.3",
		"unist-util-visit-parents": "^2.0.1"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"cross-env": "^5.0.5"
